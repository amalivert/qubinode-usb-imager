menuentry '[QUBINODE] {{ iso_file }} {{ ks_file }}' --class red --class gnu-linux --class gnu --class os --unrestricted $menuentry_id_option 'gnulinux-0-rescue-1164b42ee2a446838955e3ac703ef3a7-advanced-78eedcee-1094-4257-92e3-4c9cbf7ac6c6' {	
  load_video
	insmod gzio
	insmod part_msdos
	insmod xfs
  set ISO=/boot/iso/{{ iso_file }}
  set KS=/boot/{{ ks_file }}
  set FSUUID={{ dev_fsuuid.stdout }}
  search --fs-uuid --set=root $FSUUID
  #set root='hd0,msdos'
  loopback loop $ISO
  linuxefi /vmlinuz-3.10.0-1127.el7.x86_64 root=/dev/mapper/rhel-root ro inst.stage2=hd:UUID=$FSUUID:$ISO inst.repo=hd:UUID=$FSUUID:$ISO inst.ks=hd:UUID=$FSUUID:$KS rhgb quiet
  initrdefi (loop)/isolinux/initrd.img
}

menuentry "Boot from next device"  {
 load_video
 exit
}
if [ "x$default" = 'Red Hat Enterprise Linux Server (3.10.0-1127.el7.x86_64) 7.8 (Maipo)' ]; then default='Advanced options for Red Hat Enterprise Linux Server>Red Hat Enterprise Linux Server (3.10.0-1127.el7.x86_64) 7.8 (Maipo)'; fi;
